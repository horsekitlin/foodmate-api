{
  "name": "chat_room",
  "version": "1.0.0",
  "description": "a simple chat room",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "test:watch": "jest --watch test --color",
    "test:CI": "CI=true jest test --color --reporters=jest-junit --forceExit --runInBand --coverage --coverageDirectory=output/coverage/jest"
  },
  "keywords": [
    "chat"
  ],
  "author": "huli",
  "license": "ISC",
  "jest": {
    "testURL": "http://localhost/",
    "collectCoverageFrom": [
      "routes/*.js",
      "!src/app.js",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "text",
      "html"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-redis": "^3.4.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "express-session": "^1.16.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "promise-mysql": "^4.0.4",
    "randomstring": "^1.1.5",
    "sql-template-strings": "^2.2.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-core": "7.0.0-bridge.0",
    "dotenv": "^8.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest-jenkins-reporter": "^1.0.2",
    "jest-junit": "^6.4.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
